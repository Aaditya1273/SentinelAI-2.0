# SentinelAI 4.0 PRODUCTION Environment Variables
# Copy this to .env.local and add your REAL API keys for production launch

# BLOCKCHAIN & WALLET APIs (REQUIRED FOR REAL DATA)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
NEXT_PUBLIC_INFURA_API_KEY=your_infura_api_key_here
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id_here
PRIVATE_KEY=your_private_key_for_deployment
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_KEY
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/YOUR_KEY

# REAL MARKET DATA APIs (REQUIRED)
NEXT_PUBLIC_COINGECKO_API_KEY=your_coingecko_pro_api_key_here
NEXT_PUBLIC_DEFIPULSE_API_KEY=your_defipulse_api_key_here
NEXT_PUBLIC_DEFILLLAMA_API_KEY=your_defillama_api_key_here
COINBASE_PRO_API_KEY=your_coinbase_pro_api_key_here
COINBASE_PRO_SECRET=your_coinbase_pro_secret_here
COINBASE_PRO_PASSPHRASE=your_coinbase_pro_passphrase_here

# AI/ML APIs (REQUIRED FOR ELIZAOS)
OPENAI_API_KEY=your_openai_api_key_here
NEXT_PUBLIC_ELIZA_API_KEY=your_elizaos_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# REAL DEFI PROTOCOL APIs
SNAPSHOT_API_KEY=your_snapshot_api_key_here
THEGRAPH_API_KEY=your_thegraph_api_key_here
UNISWAP_API_KEY=your_uniswap_api_key_here
AAVE_API_KEY=your_aave_api_key_here
COMPOUND_API_KEY=your_compound_api_key_here

# PRIVACY & ZK PROOFS (REQUIRED FOR MIDNIGHT.JS)
MIDNIGHT_API_KEY=your_midnight_network_api_key_here
SNARK_PROVING_KEY=your_snark_proving_key_here
ZK_CIRCUIT_KEY=your_zk_circuit_key_here

# PRODUCTION DATABASE (REQUIRED)
DATABASE_URL=postgresql://user:password@localhost:5432/sentinelai_prod
REDIS_URL=redis://localhost:6379
MONGODB_URI=mongodb://localhost:27017/sentinelai_prod

# REAL-TIME DATA FEEDS
WEBSOCKET_URL=wss://ws-feed.pro.coinbase.com
BINANCE_WS_URL=wss://stream.binance.com:9443/ws
KRAKEN_WS_URL=wss://ws.kraken.com

# MCP & ELIZAOS SERVER
MCP_SERVER_URL=ws://localhost:8080
ELIZA_SERVER_URL=http://localhost:3000

# PRODUCTION DEPLOYMENT
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
VERCEL_TOKEN=your_vercel_token_here
NETLIFY_TOKEN=your_netlify_token_here

# ANALYTICS & MONITORING
SENTRY_DSN=your_sentry_dsn_here
MIXPANEL_TOKEN=your_mixpanel_token_here
GOOGLE_ANALYTICS_ID=your_ga_id_here

# EMAIL & NOTIFICATIONS
SENDGRID_API_KEY=your_sendgrid_api_key_here
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
